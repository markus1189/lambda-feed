name:                feeds-test
version:             0.1.0.0
license-file:        LICENSE
author:              Markus Hauck
maintainer:          markus1189@gmail.com
build-type:          Simple
cabal-version:       >=1.10

executable feeds-test
  main-is:             Main.hs
  ghc-options:         -threaded
  build-depends:       SHA >= 1.6 && < 1.7
                     , acid-state >= 0.12 && < 0.13
                     , base >=4.8 && <4.9
                     , bytestring
                     , containers
                     , feed >= 0.3 && < 0.4
                     , lens >= 4.7 && < 4.8
                     , mtl
                     , safecopy >= 0.8 && < 0.9
                     , tables >= 0.4 && < 0.5
                     , text
                     , utf8-string
                     , vty >= 5.2 && < 5.3
                     , vty-ui >= 1.8 && < 1.9
                     , wreq >= 0.3 && < 0.4
  default-language:    Haskell2010